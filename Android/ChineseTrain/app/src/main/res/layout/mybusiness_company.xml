<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include layout="@layout/view_top" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView3"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_2" />

                <cn.ieauto.autogroup.android.widget.MyEditText
                    android:id="@+id/mybusiness_company_CompanyName"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/edit_but"
                    android:ems="10"
                    tools:ignore="TextFields" />
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView4"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_3" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:orientation="horizontal" >

                    <cn.ieauto.autogroup.android.widget.MyEditText
                        android:id="@+id/mybusiness_company_Businesslicensenumber"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/edit_but"
                        android:ems="10"
                        tools:ignore="TextFields" />

                    <Button
                        android:id="@+id/mybusiness_company_button2"
                        style="@style/tv_content_17_white"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/button_e1_009"
                        android:text="@string/mybusiness_company_7" />
                </LinearLayout>
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView5"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_4" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:orientation="horizontal" >

                    <cn.ieauto.autogroup.android.widget.MyEditText
                        android:id="@+id/mybusiness_company_Address"
                        android:layout_width="0dp"
                        android:layout_height="35dp"
                        android:layout_weight="1"
                        android:background="@drawable/edit_but"
                        android:ems="10"
                        tools:ignore="TextFields" />

                    <Button
                        android:id="@+id/mybusiness_company_button3"
                        style="@style/tv_content_14_white"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/button_e1_009"
                        android:text="@string/mybusiness_company_8" />
                </LinearLayout>
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="2dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView6"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_5" />

                <cn.ieauto.autogroup.android.widget.MyEditText
                    android:id="@+id/mybusiness_company_phone"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/edit_but"
                    android:ems="10"
                    tools:ignore="TextFields" />
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView9"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_9" />

                <Button
                    android:id="@+id/mybusiness_company_button4"
                    style="@style/tv_content_17_white"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_e1_009"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_9" />
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView7"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/serviceproviders_time" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <Spinner
                        android:id="@+id/mybusiness_company_spinner1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="2dp"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_e1_009"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#fff" />

                    <Spinner
                        android:id="@+id/mybusiness_company_spinner2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="2dp"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_e1_009"
                        android:gravity="center" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_weight="0.5"
                        android:background="@color/black"
                        tools:ignore="HardcodedText" />

                    <Spinner
                        android:id="@+id/mybusiness_company_spinner3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="2dp"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_e1_009"
                        android:gravity="center" />

                    <TextView
                        style="@style/tv_content_17_black"
                        android:layout_width="1dp"
                        android:layout_height="match_parent" />

                    <Spinner
                        android:id="@+id/mybusiness_company_spinner4"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="2dp"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_e1_009"
                        android:gravity="center" />
                </LinearLayout>
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="2dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView10"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_11" />

                <cn.ieauto.autogroup.android.widget.MyEditText
                    android:id="@+id/mybusiness_company_Charges"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/edit_but"
                    android:ems="10"
                    tools:ignore="TextFields" />
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="2dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView11"
                    style="@style/tv_content_17_black"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="@string/mybusiness_company_12" />

                <cn.ieauto.autogroup.android.widget.MyEditText
                    android:id="@+id/mybusiness_company_Readme"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/edit_but"
                    android:ems="10"
                    tools:ignore="TextFields" />
            </LinearLayout>

            <Button
                android:id="@+id/mybusiness_company_Submit"
                style="@style/tv_content_17_white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/button_ok"
                android:text="@string/mybusiness_company_10" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>