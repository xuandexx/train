<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include layout="@layout/view_top" />

    <include layout="@layout/ieauto_autogroup_text_gray" />

    <LinearLayout
        android:id="@+id/RelativeLayout2"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/e4_2_081_7"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/Query_spinner1_city"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="20dp"
            android:gravity="center"
            android:text="@string/Query_1"
            android:textColor="@color/gray_normal"
            android:textSize="16sp" />
    </LinearLayout>

    <include layout="@layout/ieauto_autogroup_text_gray" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="10dp" >

        <TextView
            android:id="@+id/textView3"
            style="@style/tv_content_17_black"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/Query_2" />

        <TextView
            android:id="@+id/Query_car_spinner2"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/button_e1_009"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="16sp" />
    </LinearLayout>

    <include layout="@layout/ieauto_autogroup_horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView4"
            style="@style/tv_content_17_black"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/Query_3" />

        <TextView
            android:id="@+id/Query_spinner3_province"
            style="@style/tv_content_14_white"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/button_e1_009"
            android:gravity="center"
            android:text="选择省份" />

        <TextView
            android:id="@+id/Query_spinner4_city"
            style="@style/tv_content_14_white"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="1dp"
            android:layout_weight="1"
            android:background="@drawable/button_e1_009"
            android:gravity="center"
            android:text="选择字母" />

        <cn.ieauto.autogroup.android.widget.MyEditText
            android:id="@+id/Query_number_ed"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@drawable/edit_but"
            android:ems="10"
            android:inputType="number"
            tools:ignore="TextFields,InefficientWeight" >
        </cn.ieauto.autogroup.android.widget.MyEditText>
    </LinearLayout>

    <include layout="@layout/ieauto_autogroup_horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="10dp" >

        <TextView
            android:id="@+id/textView5"
            style="@style/tv_content_17_black"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/illegalhandl_dispel_4" />

        <cn.ieauto.autogroup.android.widget.MyEditText
            android:id="@+id/Query_GeneratorNo_ed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/edit_but"
            android:ems="10"
            tools:ignore="InefficientWeight,TextFields" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/ic_preference_last_normal"
        android:orientation="horizontal"
        android:visibility="gone" >

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/illegalhandl_dispel_3" />

        <cn.ieauto.autogroup.android.widget.MyEditText
            android:id="@+id/Query_ChassisNumber_ed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/edit_but"
            android:ems="10"
            tools:ignore="TextFields" />
    </LinearLayout>

    <Button
        android:id="@+id/Query_button"
        style="@style/tv_content_17_white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:background="@drawable/button_ok"
        android:text="@string/Query_btn" />

</LinearLayout>