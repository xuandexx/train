<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eee"
    android:orientation="vertical" >

    <include
        android:id="@+id/chengshenghuo_view_top"
        layout="@layout/view_top" />

    <ScrollView
        android:id="@+id/carlifeFargment_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/carlifeFargment_linear4"
        android:layout_below="@+id/chengshenghuo_view_top"
        android:layout_marginBottom="5dp" >

        <LinearLayout
            android:id="@+id/RelativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="416dp"
            android:layout_gravity="top"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/ll_cheshenghuo_cheyouquan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginTop="15dp"
                android:background="#fff"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="5dp"
                tools:ignore="UseCompoundDrawables" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/riderscircle" />

                <TextView
                    style="@style/tv_title_21_black"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/cheyouquan" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right" >

                    <TextView
                        style="@style/tv_title_21_black"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/red_circle"
                        android:textColor="@android:color/white"
                        android:textSize="12sp"
                        android:visibility="invisible" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:src="@drawable/mm_submenu_right" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/carlifeFargment_linear2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginTop="15dp"
                android:background="#fff"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="5dp"
                tools:ignore="UseCompoundDrawables" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/riders_assistance" />

                <TextView
                    style="@style/tv_title_21_black"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/carlife_fargment_top_2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right" >

                    <TextView
                        style="@style/tv_title_21_black"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/red_circle"
                        android:textColor="@android:color/white"
                        android:textSize="12sp"
                        android:visibility="invisible" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:src="@drawable/mm_submenu_right" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/carlifeFargment_linear3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0.1dp"
                android:background="#fff"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="5dp" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/issued_help"
                    tools:ignore="ContentDescription" />

                <TextView
                    style="@style/tv_title_21_black"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/carlife_fargment_top_3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/corlife_ll_a_bailout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="10dp"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <Button
                            android:id="@+id/carlifeFargment_button1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@drawable/e1_009_1"
                            android:gravity="center" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <Button
                            android:id="@+id/carlifeFargment_button2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@drawable/e1_009_2"
                            android:gravity="center" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" >

                        <Button
                            android:id="@+id/carlifeFargment_button3"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@drawable/e1_009_3"
                            android:gravity="center" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <Button
                            android:id="@+id/carlifeFargment_button4"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@drawable/e1_009_4"
                            android:gravity="center" />
                    </LinearLayout>
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:background="@android:color/transparent"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_5" />

                    <Button
                        android:id="@+id/carlifeFargment_button5"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_service_3" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_6" />

                    <Button
                        android:id="@+id/carlifeFargment_button6"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_request_2" />
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:background="@android:color/transparent"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_7" />

                    <Button
                        android:id="@+id/carlifeFargment_button7"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_service_4" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_8" />

                    <Button
                        android:id="@+id/carlifeFargment_button8"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_request_3" />
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:background="@android:color/transparent"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_9" />

                    <Button
                        android:id="@+id/carlifeFargment_button9"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_service_5" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_10" />

                    <Button
                        android:id="@+id/carlifeFargment_button10"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_request_4" />
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:background="@android:color/transparent"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_13" />

                    <Button
                        android:id="@+id/carlifeFargment_button13"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_request_7" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_12" />

                    <Button
                        android:id="@+id/carlifeFargment_button12"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_request_5" />
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:background="@android:color/transparent"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_15" />

                    <Button
                        android:id="@+id/carlifeFargment_button15"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_no_2" />

                    <ImageView
                        android:layout_width="45dp"
                        android:layout_height="match_parent"
                        android:contentDescription="@string/cheyouxin"
                        android:src="@drawable/e1_009_14" />

                    <Button
                        android:id="@+id/carlifeFargment_button14"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="center"
                        android:text="@string/carlife_fargment_request_6" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/carlifeFargment_linear4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#fff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="10dp" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/providing_service_life"
            tools:ignore="ContentDescription" />

        <TextView
            style="@style/tv_content_17_black"
            android:layout_width="0dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/carlife_fargment_application" />
    </LinearLayout>
    <View 
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#808080"/>
</RelativeLayout>