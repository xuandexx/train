                        <TextView
                            style="@style/tv_content_14_gar"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text="@string/comment_impression_7" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_radio8"
                        android:layout_width="0dp"
