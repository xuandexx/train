<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include layout="@layout/view_top" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <include layout="@layout/ieauto_autogroup_text_gray" />

            <RelativeLayout
                android:id="@+id/relativeLayout1"
                android:layout_width="wrap_content"
                android:layout_height="60dp" >

                <ImageView
                    android:id="@+id/Personalinformation_imageview"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:contentDescription="@string/cheyouxin"
                    android:src="@drawable/of_line_face" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/Personalinformation_imageview"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/Personalinformation_name"
                            style="@style/tv_content_17_black"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:maxLines="1"
                            android:text="@string/生意内容"
                            tools:ignore="NestedWeights" />

                        <RelativeLayout
                            android:id="@+id/Personalinformation_layout_sex"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="20dp"
                            android:background="@drawable/man_1" >

                            <TextView
                                android:id="@+id/Personalinformation_tv_age"
                                style="@style/tv_content_14_white"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="10dp"
                                android:text="23"
                                tools:ignore="HardcodedText" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            style="@style/tv_content_14_black"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@string/Personalinformation_Remarksname" />

                        <TextView
                            android:id="@+id/Personalinformation_Remarksname"
                            style="@style/tv_content_14_black"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:text="@string/remark"
                            tools:ignore="NestedWeights" />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

            <include layout="@layout/ieauto_autogroup_text_gray" />

            <LinearLayout
                android:id="@+id/LinearLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView1"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/Personalinformation_title_1" />

                    <TextView
                        android:id="@+id/Personalinformation_where"
                        style="@style/tv_content_14_black"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/生意内容" />
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView4"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/Personalinformation_title_2" />

                    <TextView
                        android:id="@+id/Personalinformation_jm"
                        style="@style/tv_content_14_black"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/生意内容" />
                </LinearLayout>

                <include layout="@layout/ieauto_autogroup_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/textView5"
                        style="@style/tv_content_17_black"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/Personalinformation_title_3" />

                    <TextView
                        android:id="@+id/Personalinformation_phonrnumber"
                        style="@style/tv_content_14_black"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/生意内容" />
                </LinearLayout>
            </LinearLayout>

            <include layout="@layout/ieauto_autogroup_text_gray" />

            <RelativeLayout
                android:id="@+id/RelativeLayout"
                android:layout_width="match_parent"
                android:layout_height="60dp" >

                <TextView
                    android:id="@+id/textView8"
                    style="@style/tv_content_17_black"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:gravity="center_vertical"
                    android:text="@string/Personalinformation_title_5" />

                <ImageView
                    android:id="@+id/Personalinformation_imageview3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView8"
                    android:contentDescription="@string/cheyouxin"
                    android:src="@drawable/of_line_face" />
            </RelativeLayout>

            <include layout="@layout/ieauto_autogroup_text_gray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="60dp" >

                <TextView
                    android:id="@+id/textView10"
                    style="@style/tv_content_17_black"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:gravity="center_vertical"
                    android:text="@string/Personalinformation_title_6" />

                <TextView
                    android:id="@+id/Personalinformation_BUSS"
                    style="@style/tv_content_14_black"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@+id/textView10"
                    android:gravity="center"
                    android:text="@string/生意内容" />
            </RelativeLayout>

            <include layout="@layout/ieauto_autogroup_horizontal" />

            <Button
                android:id="@+id/Personalinformation_button2"
                style="@style/tv_content_17_white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/button_ok"
                android:text="@string/Personalinformation_title_4" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>